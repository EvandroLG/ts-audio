    "prepack": "tsc -d"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@size-limit/preset-small-lib": "^8.1.2",
    "@types/jest": "^29.2.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "2.8.0",
    "size-limit": "^8.1.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "keywords": [
    "audio",
    "song",
    "playlist",
    "AudioContext"
  ],
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "1 KB"
    }
  ]
}
