{
  "name": "ts-audio",
  "version": "0.7.2",
  "author": "evandrolg",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "size": "yarn build && size-limit",
    "test": "jest",
    "lint": "eslint src/",
    "build": "rm -rf dist/* && tsc -d",
